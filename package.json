{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run vite\" \"npm run tsc\"",
        "serve": "concurrently \"npm run vite\" \"npm run tsc\"",
        "prod": "vite build --mode production",
        "vite": "vite --https --host vite.quizz.test --port 4443 --clearScreen false",
        "tsc": "tsc -w --preserveWatchOutput",
        "eslint": "node ./node_modules/.bin/eslint --max-warnings 0 -c .eslintrc.json ./resources",
        "eslint-fix": "node ./node_modules/.bin/eslint --fix --max-warnings 0 -c .eslintrc.json ./resources",
        "eslint-ci": "node ./node_modules/.bin/eslint --max-warnings 0 -c .eslintrc.json ./resources -f junit",
        "stylelint": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --max-warnings 0 '**/resources/**/*.scss'",
        "stylelint-fix": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --fix --max-warnings 0 '**/resources/**/*.scss'",
        "stylelint-ci": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --max-warnings 0 '**/resources/**/*.scss' -f json"
    },
